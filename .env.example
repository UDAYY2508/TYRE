# EmailJS Configuration
# Copy this file to .env and fill in your EmailJS credentials

# Your EmailJS Service ID (found in Email Services dashboard)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# Your EmailJS Template ID (found in Email Templates dashboard)
VITE_EMAILJS_TEMPLATE_ID=your_template_id_here

# Your EmailJS Public Key (found in Account > API Keys)
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Email address where appointment requests will be sent
VITE_RECEIVER_EMAIL=your-email@example.com
